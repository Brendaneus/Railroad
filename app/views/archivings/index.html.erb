<% if admin_user? %>
	<nav class="admin post_control">
		<%= link_to "New Archiving", new_archiving_path %>
	</nav>
<% end %>

<% if @archivings.empty? %>
	<h2>There isn't anything here...</h2>
<% else %>

	<% @archivings.each do |archiving| %>
		<div class="container">
			<h2><%= link_to archiving.title, archiving_path(archiving) %></h2>
		</div>
	<% end %>

<% end %>