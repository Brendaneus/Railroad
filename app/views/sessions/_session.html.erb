<%= link_to user_session_path(session.user, session), class: container_class(session) do %>
	<h2><%= session.name %></h2>

	<% if session.ip.present? %>

		<h4 class="subtle right">Last IP: <%= session.ip %></h4>

	<% end %>

	<%= render 'shared/timestamps', object: session, activity: true %>
<% end %>