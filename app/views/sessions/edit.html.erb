<% content_for :header_extension do %>
	/ <%= link_to "Users", users_path %>
	<% if @user.trashed? %>
		/ <%= link_to "Trash", trashed_users_path %>
	<% end %>
	/ <%= link_to @user.name, user_path(@user) %>
	/ <%= link_to "Sessions", user_sessions_path(@user) %>
	/ <%= link_to @session.name, user_session_path(@user, @session) %>
	/ <%= link_to "Edit", edit_user_session_path(@user, @session) %>
<% end %>

<%= render 'shared/error_messages', object: @session %>

<%= render 'form', form_path: user_session_path(@user, @session), submit_text: "Update Session" %>

<%= render 'shared/timestamps', object: @session, create_alias: "Created" %>